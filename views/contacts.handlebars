                            </tr>
                        </thead>
                        <tbody id="Specialists_table">

                        </tbody>

                    </table>
                </div>


            </div>






            <div class="tab-pane" id="Customers" role="tabpanel" aria-labelledby="Customers-tab"><br>
                <button class="btn btn-info mb-2" data-toggle="modal" data-target=".bd-example-modal-lg1">
                    <i class="far fa-plus-square"></i> New Customer
                </button>
                <br>
                <div class="table-responsive-sm">
                    <table class="table table-hover">
                        <thead style="background-color:#7a6eb8" class="text-light">
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">First Name</th>
                                <th scope="col">Last Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Phone</th>
                                <th scope="col">Address</th>
                                <th scope="col">City</th>
                                <th scope="col">State</th>
                                <th scope="col">Zip Code</th>
                                <th scope="col"></th>



                            </tr>
                        </thead>
                        <tbody id="Customers_table">

                        </tbody>

                    </table>
                </div>
            </div>
        </div>
