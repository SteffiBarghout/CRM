        <h2 class="display-4"><i class="fas fa-address-card"></i> Contacts</h2>
        <ul class="nav nav-tabs nav-fill" id="myTab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="Specialists-tab" data-toggle="tab" href="#Specialists" role="tab"
                    aria-controls="Specialists" aria-selected="true">Specialists</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Customers-tab" data-toggle="tab" href="#Customers" role="tab"
